{
  "manifest_version": 3,
  "name": "Yobama Image Replacer",
  "version": "1.0",
  "action": {
    "default_popup": "hello.html",
    "default_icon": "yobama.png"
    },
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ]
}